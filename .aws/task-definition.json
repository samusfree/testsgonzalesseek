{
  "family": "testseek",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "859741487195.dkr.ecr.us-west-2.amazonaws.com/seek-dev:3b89dee4261b9b8b3e9dfbdf47e2379dc2e26c81",
      "cpu": 0,
      "portMappings": [
        {
          "name": "8080",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "SEEK_DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:859741487195:secret:seeksecret-tIdoCg:SEEK_DB_PASSWORD::"
        },
        {
          "name": "SEEK_DB_URL",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:859741487195:secret:seeksecret-tIdoCg:SEEK_DB_URL::"
        },
        {
          "name": "SEEK_DB_USER",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:859741487195:secret:seeksecret-tIdoCg:SEEK_DB_USER::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/testseek",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "healthCheck": {
        "command": [
          "curl localhost:8080/actuator/health"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::859741487195:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::859741487195:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}